# Runtime
APP_NAME=market_data
ENV=prod
LOG_LEVEL=info
HOST=0.0.0.0
PORT=8000

# CORS, add your Mini App origin if needed
# Example: CORS_ALLOW_ORIGINS='["https://mydomain.com"]'
CORS_ALLOW_ORIGINS=[]

# Upstreams
EODHD_BASE_URL=https://eodhd.com/api
EODHD_API_TOKEN=REPLACE_WITH_YOUR_TOKEN
FX_BASE_URL=http://fx:8000

# DB and TTLs
DB_PATH=/app/data/cache.db
QUOTES_TTL_SEC=90
BENCH_TTL_SEC=900
META_TTL_SEC=86400

# Telegram
TELEGRAM_BOT_TOKEN=
